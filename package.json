{
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "customEditors": [
      {
        "displayName": "DesAsm Preview",
        "selector": [
          {
            "filenamePattern": "*.ds"
          }
        ],
        "priority": "option",
        "viewType": "dasm-preview"
      }
    ],
    "grammars": [
      {
        "language": "dsm",
        "path": "./syntaxes/dsm.tmLanguage.json",
        "scopeName": "source.ds"
      }
    ],
    "languages": [
      {
        "aliases": [
          "Dsm",
          "dsm"
        ],
        "extensions": [
          ".ds"
        ],
        "id": "dsm",
        "configuration": "./language-configuration.json"
      }
    ],
    "configuration":{
      "title": "",
      "properties": {
        "dsmLang.desmosEngineOffline": {
          "type": "boolean",
          "default": true,
          "description": "if true, the desmos engine used by dsm intrepreter will be an offline version of the desmos api"
        },
        "dsmLAng.desmosEngineVersion": {
          "type": "string",
          "default": "1.8",
          "description": "the version of the desmos engined used by the dsm interpreter. currently only 1.8 and 1.4 are supported"
        }
      }
    }
  },
  "description": "Language Interpreter & Support for Dsm.",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "displayName": "Dsm-Language",
  "engines": {
    "vscode": "^1.83.0"
  },
  "main": "./out/extension.js",
  "name": "Dsm-Language",
  "publisher": "octopus-writer",
  "repository": {
    "url": "https://github.com/yosefgoren/Desmos-Calculator-Window.git"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.2.0"
}